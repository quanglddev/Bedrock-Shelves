  pod 'FloatRatingView', '~> 2.0.0'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'SCLAlertView'
  pod 'MarqueeLabel/Swift'
  pod 'Armchair', '>= 0.3'
  
  
  target 'bedrockbookTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'bedrockbookUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
